{
  "name": "core-styles-3",
  "version": "3.0.0-beta-14",
  "description": "Design System for Webstop's grocery websites.",
  "author": "Greg Hemphill",
  "license": "private",
  "private": true,
  "scripts": {
    "docs-build": "hugo --cleanDestinationDir",
    "css": "npm-run-all --parallel css-compile",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css scss/:dist/css/",
    "css-docs": "npm-run-all --parallel css-docs-compile",
    "css-docs-compile": "sass --style expanded --source-map --embed-sources --no-error-css site/assets/scss/:site/static/css/",
    "js": "npm-run-all js-clear js-compile js-minify",
    "js-clear": "rm -rf dist/core-styles/js",
    "js-compile": "npm-run-all --aggregate-output --parallel js-compile-*",
    "js-minify": "npm-run-all --aggregate-output --parallel js-minify-*",
    "js-main": "npm-run-all js-compile-main js-minify-main",
    "js-compile-main": "rollup js/main.js --file dist/js/core-styles.js --config build/rollup.config.mjs --format iife --sourcemap",
    "js-minify-main": "terser dist/js/core-styles.js --compress --output dist/js/core-styles.min.js",
    "js-milkdown": "npm-run-all js-compile-milkdown js-minify-milkdown",
    "js-compile-milkdown": "rollup js/milkdown.js --config build/rollup.milkdown.config.mjs --file dist/js/milkdown.js --format iife --sourcemap",
    "js-minify-milkdown": "terser dist/js/milkdown.js --compress --output dist/js/milkdown.min.js",
    "js-docs": "npm-run-all js-docs-compile js-docs-minify",
    "js-docs-compile": "npm-run-all --aggregate-output --parallel js-docs-compile-*",
    "js-docs-minify": "npm-run-all --aggregate-output --parallel js-docs-minify-*",
    "js-docs-main": "npm-run-all js-docs-compile-main js-docs-minify-main",
    "js-docs-compile-main": "rollup js/main.js --file site/static/js/core-styles.js --config build/rollup.config.mjs --format iife --sourcemap",
    "js-docs-minify-main": "terser site/static/js/core-styles.js --compress --output site/static/js/core-styles.min.js",
    "js-docs-milkdown": "npm-run-all js-docs-compile-milkdown js-docs-minify-milkdown",
    "js-docs-compile-milkdown": "rollup js/milkdown.js --config build/rollup.milkdown.config.mjs --file site/static/js/milkdown.js --format esm --sourcemap",
    "js-docs-minify-milkdown": "terser site/static/js/milkdown.js --compress --output site/static/js/milkdown.min.js",
    "js-docs-compile-milkdown-try-m": "rollup js/milkdown-try-m.js --config build/rollup.milkdown.config.mjs --file site/static/js/milkdown-try-m.js --format esm --sourcemap",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon -L --watch scss/ --ext scss --exec \"npm-run-all css-compile\"",
    "watch-css-docs": "nodemon -L --watch site/assets/scss/ --ext scss --exec \"npm run css-compile-docs\"",
    "watch-js": "nodemon -L --watch js/ --ext scss --exec \"npm-run-all js js-docs\"",
    "clear": "rm -rf dist/",
    "dist": "npm-run-all --parallel clear js css",
    "docs": "npm-run-all --parallel js-docs css-docs",
    "pushable": "npm-run-all --parallel dist docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webstop/core-styles-3.git"
  },
  "homepage": "https://guides.webstop.com/",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@milkdown-lab/plugin-menu": "",
    "@milkdown/core": "7.3.5",
    "@milkdown/ctx": "7.3.5",
    "@milkdown/plugin-upload": "7.3.5",
    "@milkdown/preset-commonmark": "7.3.5",
    "@milkdown/preset-gfm": "7.3.5",
    "@milkdown/prose": "7.3.5",
    "@milkdown/theme-nord": "7.3.5",
    "@milkdown/transformer": "7.3.5",
    "@popperjs/core": "^2.11.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "ahoy.js": "^0.4.3",
    "bootstrap": "^5.3.2",
    "bootstrap-5-docs": "github:hemphill/bootstrap-5-docs#main",
    "hugo-bin": "^0.85.3",
    "jquery": "^3.7.0",
    "js-cookie": "^3.0.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.13.0",
    "sass": "^1.50.1",
    "terser": "^5.29.2"
  }
}
